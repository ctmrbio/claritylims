            <xsd:enumeration value="HCV"/>
            <xsd:enumeration value="HDV"/>
            <xsd:enumeration value="HEV"/>
        <!-- <xsd:enumeration value="H1N1l"/> -->
            <xsd:enumeration value="H5N1l"/>
