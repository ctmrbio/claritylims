            <xsd:enumeration value="HIV"/>
            <xsd:enumeration value="HTLV1"/>
            <xsd:enumeration value="HTLV2"/>
           <!-- <xsd:enumeration value="Infl"/> -->
            <xsd:enumeration value="ITypA"/>
