        <xsd:annotation>
            <xsd:documentation xml:lang="se">
                Definierar vilken laboratoriediagnos som avses anmälas.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:string">
        	<xsd:enumeration value="Apato"/>
            <xsd:enumeration value="Banthr"/>
            <xsd:enumeration value="Bpertus"/>
            <xsd:enumeration value="Bruspp"/>
            <xsd:enumeration value="Campyl"/>
            <xsd:enumeration value="Cbotul"/>
            <xsd:enumeration value="Cburne"/>
            <xsd:enumeration value="Cdipht"/>
