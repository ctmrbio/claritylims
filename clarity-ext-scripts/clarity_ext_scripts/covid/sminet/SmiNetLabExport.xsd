        </xsd:restriction>
    </xsd:simpleType>

    <!-- ******************* Specification ****************** -->
    <xsd:complexType name="SpecificationType">
        <xsd:sequence>
            <!--Namnet på den information som skall specificeras, exempelvis "Fagtyp". -->
			<!--Observera att det är SmiNet-koden för specifikationen som ska användas. -->
            <xsd:element name="key" type="LimitedString"
                minOccurs="1" maxOccurs="1"/>
